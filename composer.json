{
    "name": "contao/installation-bundle",
    "type": "contao-bundle",
    "description": "Contao 4 installation bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/filesystem": "^3.2",
        "symfony/framework-bundle": "^3.2",
        "contao/core-bundle": "dev-develop",
        "sensio/distribution-bundle": "^5.0",
        "sensiolabs/ansi-to-html": "^1.1"
    },
    "require-dev": {
        "contao/manager-plugin": "^1.0",
        "friendsofphp/php-cs-fixer": "^1.8"
    },
    "conflict": {
        "contao/core": "*"
    },
    "autoload": {
        "psr-4": {
            "Contao\\InstallationBundle\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.3.x-dev"
        },
        "contao-manager-plugin": "Contao\\InstallationBundle\\ContaoManager\\Plugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
